    "dompurify": "^3.0.9",
    "filedrop-svelte": "^0.1.2",
    "filesize": "^10.1.1",
    "install": "^0.13.0",
    "libphonenumber-js": "^1.10.58",
    "marked": "^12.0.1",
    "npm": "^10.5.0",
    "page": "^1.11.6",
    "postcss-importantly": "^0.0.7",
    "serve": "^14.2.1",
    "socket.io-client": "^4.7.4",
